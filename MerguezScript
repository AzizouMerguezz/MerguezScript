local function teleportPlayer(position)
    local player = game.Players.LocalPlayer

    -- V√©rifier si le joueur et son personnage existent
    if player and player.Character then
        local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
        
        -- V√©rifier si HumanoidRootPart est trouv√©
        if humanoidRootPart then
            -- Ajouter un d√©calage vers le haut pour √©viter les collisions avec le sol
            local targetPosition = position + Vector3.new(0, 5, 0) -- Ajustement de 5 unit√©s en hauteur
            
            -- Effectuer la t√©l√©portation
            humanoidRootPart.CFrame = CFrame.new(targetPosition)
            print("T√©l√©portation r√©ussie vers : " .. tostring(targetPosition))
        else
            warn("HumanoidRootPart introuvable pour le joueur : " .. player.Name)
        end
    else
        warn("Le joueur ou le personnage n'est pas pr√™t.")
    end
end

-- Coordonn√©es des √Æles

local Button = TPTab:CreateToggle({
    Name = "üåÄ   Spin",
    Callback = function()
        -- Toggle pour activer/d√©sactiver la rotation rapide
        _G.spinMode = not _G.spinMode

        if _G.spinMode then
            -- Notifie que le mode toupie est activ√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "Spin Enable!",
                Duration = 5
            })

            -- R√©f√©rence au joueur et √† son HumanoidRootPart
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

            -- Fonction pour faire tourner le personnage
            local function spinCharacter()
                while _G.spinMode do
                    humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.Angles(0, math.rad(250), 0)  -- Rotation rapide autour de l'axe Y
                    wait(0.05)  -- Attendre un court instant entre les rotations
                end
            end

            -- D√©marre la rotation
            spinCharacter()

        else
            -- Notifie que le mode toupie est d√©sactiv√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "Spin Disable!",
                Duration = 5
            })
        end
    end,
})


local UserInputService = game:GetService("UserInputService")

local UserInputService = game:GetService("UserInputService")

local Button = TPTab:CreateToggle({ 
    Name = "üìç   CTRL TP",
    Callback = function()
        -- Toggle pour activer/d√©sactiver la t√©l√©portation
        _G.tpMode = not _G.tpMode

        if _G.tpMode then
            -- Notifie que le mode de t√©l√©portation est activ√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "TP enabled! Ctrl+Click !",
                Duration = 5
            })

            -- R√©f√©rence au joueur
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

            -- √âcouteur pour les clics de souris
            local mouse = player:GetMouse()
            mouse.Button1Down:Connect(function()
                if _G.tpMode and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
                    -- V√©rifiez si le HumanoidRootPart existe
                    if humanoidRootPart then
                        local targetPosition = mouse.Hit.Position

                        -- Utiliser un Raycast pour d√©terminer la position cible
                        local rayOrigin = targetPosition + Vector3.new(0, 100, 0) -- √âlever le rayon au-dessus du mur
                        local rayDirection = Vector3.new(0, -200, 0) -- Raycast vers le bas

                        local raycastResult = workspace:Raycast(rayOrigin, rayDirection)

                        if raycastResult then
                            -- Position du mur o√π le raycast a touch√©
                            local wallPosition = raycastResult.Position
                            humanoidRootPart.CFrame = CFrame.new(wallPosition + Vector3.new(0, 5, 0))  -- Ajuster la hauteur pour √©viter de tomber
                        end
                    end
                end
            end)

        else
            -- Notifie que le mode de t√©l√©portation est d√©sactiv√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "TP disabled!",
                Duration = 5
            })
        end
    end,
})


local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Cr√©e un ScreenGui pour afficher les coordonn√©es
local screenGui = Instance.new("ScreenGui", player.PlayerGui)
screenGui.Name = "CoordScreenGui"

-- Cr√©e un TextLabel pour afficher les coordonn√©es
local coordLabel = Instance.new("TextLabel")
coordLabel.Size = UDim2.new(0, 0, 0, 0)  -- Taille du TextLabel
coordLabel.Position = UDim2.new(0.5, 0, 0, 950)  -- Centr√© en haut de l'√©cran
--coordLabel.BackgroundColor3 = Color3.new(0, 0, 0)  -- Couleur de fond noire
coordLabel.TextColor3 = Color3.new(1, 1, 1)  -- Texte blanc
coordLabel.TextSize = 24
coordLabel.TextStrokeTransparency = 0.5
coordLabel.Text = "Coordinates: X: 0, Y: 0, Z: 0"  -- Valeurs par d√©faut
coordLabel.Parent = screenGui  -- Ajoute le TextLabel au ScreenGui
--coordLabel.Visible = false  -- Cache par d√©faut

-- Toggle pour activer/d√©sactiver l'affichage des coordonn√©es
local Button = TPTab:CreateToggle({
    Name = "üìç   Coordinates",
    Callback = function()
        _G.coordMode = not _G.coordMode  -- Alterne l'√©tat du mode

        if _G.coordMode then
            -- Notifie que l'affichage des coordonn√©es est activ√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "Coordinates enabled!",
                Duration = 5
            })

            coordLabel.Visible = true  -- Affiche le label des coordonn√©es

            -- Met √† jour les coordonn√©es en temps r√©el
            while _G.coordMode do
                wait(0.1)  -- Met √† jour toutes les 0.1 secondes
                if humanoidRootPart then
                    local position = humanoidRootPart.Position
                    coordLabel.Text = string.format("Coordinates: X: %.2f, Y: %.2f, Z: %.2f", position.X, position.Y, position.Z)
                end
            end

        else
            -- Notifie que l'affichage des coordonn√©es est d√©sactiv√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "Coordinates Disabled!",
                Duration = 5
            })

            coordLabel.Visible = false  -- Cache le label des coordonn√©es
        end
    end,
})

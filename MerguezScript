local Button = MainTab:CreateToggle({
    Name = "üõ°Ô∏è   God Mode",
    Callback = function()
        -- Toggle pour activer/d√©sactiver le God Mode
        _G.godMode = not _G.godMode

        if _G.godMode then
            -- Notifie que le God Mode est activ√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "God Mode Enable!",
                Duration = 5
            })

            -- R√©f√©rence au joueur et √† son humano√Øde
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:FindFirstChildOfClass('Humanoid')

            -- D√©finit la sant√© maximale √† l'infini
            humanoid.MaxHealth = math.huge
            humanoid.Health = humanoid.MaxHealth  -- Restaure la sant√© √† sa valeur maximale

            -- Emp√™che la mort en mettant un hook sur le changement de sant√©
            humanoid.HealthChanged:Connect(function()
                if humanoid.Health <= 0 and _G.godMode then
                    humanoid.Health = humanoid.MaxHealth  -- Restaure la sant√© si le God Mode est activ√©
                end
            end)
        else
            -- Notifie que le God Mode est d√©sactiv√©
            game.StarterGui:SetCore("SendNotification", {
                Title = "MerguezHub",
                Text = "God Mode Disable!",
                Duration = 5
            })

            -- Remets la sant√© maximale √† 100
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:FindFirstChildOfClass('Humanoid')

            if humanoid then
                humanoid.MaxHealth = 100  -- Remets la sant√© maximale √† 100
                humanoid.Health = humanoid.MaxHealth  -- Restaure la sant√© √† 100
            end
        end
    end,
})

